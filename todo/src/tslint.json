{
  "extends": "../tslint.json",
  "rules": {
    "quotemark": [
      true,
      "double",
      "jsx-double"
    ],
    "prettier": [true, { "singleQuote": true }],
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
